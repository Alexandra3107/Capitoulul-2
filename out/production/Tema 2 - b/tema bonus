        int [] a = {4, 5, 4, 6, 3, 1, 7, 9, 3, 4, 5};
        List<Integer> uniqueNumber = new ArrayList<>();
        Object [] newArray = uniqueNumbers.toArray();

        for (  int i = 0; i<a.length; i++) {
            int counter = 0;
            if(!uniqueNumbers.contains(a[i])){
                uniqueNumbers.add(a[i]);
                counter++;
                for (int j=i+1; j<a.length; j++){
                    if(a[i] == a[j]){
                        counter++;
                    }
                }
                System.out.println(a[i] + " is found " + counter+ " times in the array.");
            }
        }
        System.out.println(uniqueNumbers);
